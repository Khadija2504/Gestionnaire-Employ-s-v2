        applicatio0_.candidateName as candidat4_1_,
        applicatio0_.candidatePhone as candidat5_1_,
        applicatio0_.job_offer_id as job_offe8_1_,
        applicatio0_.resume as resume6_1_,
        applicatio0_.status as status7_1_ 
    from
        applications applicatio0_ 
    where
        applicatio0_.job_offer_id=?
